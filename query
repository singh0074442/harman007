select * from lot




select veggies.veggie_name, 
lot.quantity_of_vegatable * veggies.veggie_price 
from lot, veggies
where lot.veggie_id =
veggies.veggie_id





challenge 1
select veggies.veggie_name,
lot.quantity_of_vegatable * veggies.veggie_price
from lot, veggies
where lot.veggie_id =
veggies.veggie_id

challenge 2
select  min(lot.quantity_of_vegatable) , min(veggies.veggie_name)
from lot, veggies
where lot.veggie_id = veggies.veggie_id 
and veggies.veggie_type = 'root'

challenge 3
select veggies.veggie_name, 
lot.quantity_of_vegatable * veggies.veggie_price as total 
from lot, veggies
where lot.veggie_id =
veggies.veggie_id
order by veggie_name
Challenge 4: 
a.  TOTAL AMOUNT OF ALL VEGATABLES ON THE FARM, ALL LOTS
 
select sum(lots.quantity_of_vegatables )
from lots, veggies
where lots.veggie_id =
veggies.veggie_id
 
Challenge 5: 
https://www.tutorialspoint.com/sql/sql-string-functions.htm
TOTAL AMOUNT OF ALL VEGATABLES starting the Letter B



select veggie_name  
from field
where LEFT(veggie_name, 1) = 'B'


select veggie_name  
from field
where LEN(veggie_name) <= 4 


select * 
from veggies v 
join lot l 
ON v.veggie_id = l.veggie_id




